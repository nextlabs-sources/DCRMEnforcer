		function HiddenCrmRibbonManager()
		{
			var crmRibbon = window.parent.document.getElementById("crmTopBar");
			//crmRibbon.style["visibility"] = "hidden";
			crmRibbon.style["display"] = "none";
		}
		function ShowCrmRibbonManager()
		{
			var crmRibbon = window.parent.document.getElementById("crmTopBar");
			//crmRibbon.style["visibility"] = "visible";
			crmRibbon.style["display"] = "block";
		}
	</script>
